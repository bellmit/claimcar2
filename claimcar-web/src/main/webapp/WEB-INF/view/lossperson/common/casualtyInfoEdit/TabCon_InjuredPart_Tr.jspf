<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ include file="/WEB-INF/layout/common/taglib.jspf"%>
<tr>
	<td>
		<span class="select-box">
			<input type="hidden" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].id" value="${persExt.id }">
			<input type="hidden" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].registNo" value="${persExt.registNo }">
			<input type="hidden" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].injuredPart" value="${persExt.injuredPart }">
			<app:codeSelect codeType="InjuredPart" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].injuredPart"
				disabled="true" value="${persExt.injuredPart }" />
		</span>
	</td>
	<td>
		<span class="select-box">
			<app:codeSelect codeType="InjuredDiag" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].injuredDiag"
				value="${persExt.injuredDiag }" />
		</span>
	</td>
	<td>
		<span class="select-box">
			<app:codeSelect codeType="TreatMethod" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].treatMethod"
				value="${persExt.treatMethod }" />
		</span>
	</td>
	<td>
		<span class="select-box">
			<app:codeSelect codeType="TreatWay" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].treatWay" value="${persExt.treatWay }" />
		</span>
	</td>
	<td>
		<span class="select-box">
			<app:codeSelect codeType="ProgSituation" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].progSituation"
				value="${persExt.progSituation }" />
		</span>
	</td>
	<td>
		<span class="select-box">
			<app:codeSelect codeType="WoundGrade" type="select" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].woundGrade"
				value="${persExt.woundGrade }" />
		</span>
	</td>
	<td>
		<input type="text" class="input-text" maxlength="200" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].diagDetail" value="${persExt.diagDetail }" />
	</td>
	<td>
		<input type="text" class="input-text" maxlength="200" name="prpLDlossPersTraceVo.prpLDlossPersInjured.prpLDlossPersExts[${injuredPartSize }].trackRemark" value="${persExt.trackRemark }" />
	</td>
</tr>

