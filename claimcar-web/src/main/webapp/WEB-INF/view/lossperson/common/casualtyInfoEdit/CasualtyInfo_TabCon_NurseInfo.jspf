<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ include file="/WEB-INF/layout/common/taglib.jspf"%>
<div class="table_wrap">
	<div class="table_title f14">护理人员信息</div>
	<div class="table_cont  table_list">
		<table class="table table-border">
			<thead class="text-c">
				<tr>
					<th width="6%">
						<button type="button" class="btn btn-plus Hui-iconfont Hui-iconfont-add" name="nurseInfoAddBtn" onclick="add_NurseInfo(this)"></button>
					</th>
					<th width="10%">姓名</th>
					<th>性别</th>
					<th>职业</th>
					<th>收入</th>
					<th>与受害人关系</th>
				</tr>
			</thead>
			<tbody id="nurseInfo_Tbody" class="text-c">
				<c:forEach var="persNurses" items="${persTrace.prpLDlossPersInjured.prpLDlossPersNurses }" varStatus="nurseStatus">
					<c:set var="nurseInfoSize" value="${nurseStatus.index}" />
					<%@ include file="TabCon_NurseInfo_Tr.jspf"%>
				</c:forEach>
			</tbody>
		</table>
	</div>
</div>